<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <size android:width="@dimen/bibi_dimen_100" android:height="@dimen/bibi_dimen_30"></size>
            <corners android:radius="6dp"></corners>
            <!--<gradient android:startColor="#44419e" android:endColor="#44419e" android:centerColor="#5c59ba"></gradient>-->
            <solid android:color="#5c59ba"></solid>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <size android:width="@dimen/bibi_dimen_100" android:height="@dimen/bibi_dimen_30"></size>
            <corners android:radius="6dp"></corners>
            <solid android:color="#44419e"></solid>
        </shape>
    </item>
</selector>